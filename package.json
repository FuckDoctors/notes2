{
  "type": "module",
  "private": true,
  "packageManager": "pnpm@10.11.0",
  "package-manager-strict": false,
  "scripts": {
    "build": "pnpm docs:build",
    "dev": "pnpm docs:dev",
    "clean-dev": "pnpm docs:clean-dev",
    "lint": "eslint --cache .",
    "lint:antfu": "eslint --cache .",
    "lint:antfu-fix": "eslint --cache . --fix",
    "lint:types": "vue-tsc --noEmit --skipLibCheck",
    "lint:css": "stylelint --cache --fix \"**/*.{css,scss,vue}\" --allow-empty-input",
    "lint:js": "eslint --cache --fix \"**/*.{js,ts,vue,jsx,tsx}\"",
    "lint:md": "markdownlint --fix \"**/*.md\"",
    "lint:remark": "remark .",
    "lint:textlint": "textlint --fix",
    "lint:format": "prettier --write \"**/*.{js,ts,json,css,scss,vue,html,md}\"",
    "lint:all": "pnpm lint:css && pnpm lint:js && pnpm lint:md && pnpm lint:remark && pnpm lint:format",
    "up": "taze major -I",
    "prepare": "husky",
    "cm": "cz",
    "docs:build": "vuepress-vite build docs",
    "docs:build-ex": "cross-env NODE_OPTIONS=--max_old_space_size=8192 vuepress-vite build docs",
    "docs:clean-dev": "vuepress-vite dev docs --clean-cache",
    "docs:dev": "vuepress-vite dev docs",
    "docs:update": "pnpm dlx vp-update",
    "test": "vitest",
    "sizecheck": "cross-env SIZE_CHECK=true pnpm docs:build"
  },
  "dependencies": {
    "cnchar": "^3.2.6",
    "cnchar-draw": "^3.2.6",
    "cnchar-explain": "^3.2.6",
    "cnchar-idiom": "^3.2.6",
    "cnchar-order": "^3.2.6",
    "cnchar-radical": "^3.2.6",
    "cnchar-voice": "^3.2.6",
    "cnchar-words": "^3.2.6",
    "vue": "^3.5.26"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.19.0",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@commitlint/cz-commitlint": "^19.8.1",
    "@mathjax/src": "^4.1.0",
    "@textlint/ast-node-types": "^15.5.0",
    "@textlint/types": "^15.5.0",
    "@types/postcss-import": "^14.0.3",
    "@unocss/eslint-config": "^66.5.12",
    "@vitest/coverage-v8": "^3.2.4",
    "@vue/repl": "^4.7.1",
    "@vue/test-utils": "^2.4.6",
    "@vuepress/bundler-vite": "2.0.0-rc.26",
    "@vuepress/bundler-webpack": "2.0.0-rc.26",
    "@vuepress/plugin-clarity-analytics": "2.0.0-rc.121",
    "@vuepress/plugin-comment": "2.0.0-rc.121",
    "@vuepress/plugin-docsearch": "2.0.0-rc.121",
    "@vuepress/plugin-feed": "2.0.0-rc.121",
    "@vuepress/plugin-links-check": "2.0.0-rc.121",
    "@vuepress/plugin-llms": "2.0.0-rc.121",
    "@vuepress/plugin-markdown-hint": "2.0.0-rc.121",
    "@vuepress/plugin-markdown-image": "2.0.0-rc.121",
    "@vuepress/plugin-markdown-math": "2.0.0-rc.121",
    "@vuepress/plugin-markdown-tab": "2.0.0-rc.121",
    "@vuepress/plugin-pwa": "2.0.0-rc.121",
    "@vuepress/plugin-revealjs": "2.0.0-rc.121",
    "@vuepress/plugin-seo": "2.0.0-rc.121",
    "@vuepress/plugin-shiki": "2.0.0-rc.121",
    "@vuepress/plugin-sitemap": "2.0.0-rc.121",
    "@vuepress/plugin-watermark": "2.0.0-rc.121",
    "@vuepress/shiki-twoslash": "2.0.0-rc.121",
    "@waline/client": "^3.8.0",
    "autoprefixer": "^10.4.23",
    "commitizen": "^4.3.1",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "dotenv": "^17.2.3",
    "echarts": "^5.6.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-cypress": "^5.2.1",
    "eslint-plugin-format": "^1.2.0",
    "flowchart.ts": "^3.0.1",
    "husky": "^9.1.7",
    "katex": "^0.16.27",
    "lint-staged": "^16.2.7",
    "markdown-it": "^14.1.0",
    "markdown-it-container": "^4.0.0",
    "markdown-it-link-attributes": "^4.0.1",
    "markdownlint": "^0.38.0",
    "markdownlint-cli": "^0.45.0",
    "markmap-lib": "^0.18.12",
    "markmap-toolbar": "^0.18.12",
    "markmap-view": "^0.18.12",
    "mermaid": "^11.12.2",
    "postcss": "^8.5.6",
    "postcss-html": "^1.8.0",
    "postcss-import": "^16.1.1",
    "postcss-scss": "^4.0.9",
    "prettier": "^3.7.4",
    "remark-cli": "^12.0.1",
    "remark-gfm": "^4.0.1",
    "remark-github": "^12.0.0",
    "remark-preset-lint-consistent": "^6.0.1",
    "remark-preset-lint-recommended": "^7.0.1",
    "remark-toc": "^9.0.0",
    "remark-validate-links": "^13.1.0",
    "reveal.js": "^5.2.1",
    "rollup-plugin-visualizer": "^6.0.5",
    "sandpack-vue3": "^3.1.12",
    "sass": "^1.97.2",
    "stylelint": "^16.26.1",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-hudochenkov": "^12.0.0",
    "stylelint-config-standard": "^38.0.0",
    "stylelint-order": "^7.0.1",
    "stylelint-scss": "^6.14.0",
    "taze": "^19.9.2",
    "textlint": "^15.5.0",
    "textlint-rule-common-misspellings": "^1.0.1",
    "textlint-rule-no-todo": "^2.0.1",
    "textlint-rule-rousseau": "^1.4.8",
    "textlint-rule-unexpanded-acronym": "^1.2.4",
    "typescript": "^5.9.3",
    "unocss": "^66.5.12",
    "vitest": "^3.2.4",
    "vue-tsc": "^3.2.2",
    "vuepress": "2.0.0-rc.26",
    "vuepress-theme-hope": "2.0.0-rc.102"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@algolia/client-search",
        "react",
        "react-dom",
        "@types/react",
        "webpack"
      ]
    },
    "onlyBuiltDependencies": [
      "@parcel/watcher",
      "esbuild",
      "unrs-resolver"
    ],
    "patchedDependencies": {
      "@vuepress/plugin-clarity-analytics@2.0.0-rc.121": "patches/@vuepress__plugin-clarity-analytics@2.0.0-rc.121.patch"
    }
  }
}
