{
  "type": "module",
  "private": true,
  "packageManager": "pnpm@9.1.1",
  "package-manager-strict": false,
  "scripts": {
    "build": "pnpm docs:build",
    "dev": "pnpm docs:dev",
    "lint": "eslint .",
    "lint:antfu": "eslint .",
    "lint:antfu-fix": "eslint . --fix",
    "lint:types": "vue-tsc --noEmit --skipLibCheck",
    "lint:css": "stylelint --fix \"**/*.{css,scss,vue}\"",
    "lint:js": "eslint --fix \"**/*.{js,ts,vue,jsx,tsx}\"",
    "lint:md": "markdownlint --fix \"**/*.md\"",
    "lint:remark": "remark .",
    "lint:textlint": "textlint --fix",
    "lint:format": "prettier --write \"**/*.{js,ts,json,css,scss,vue,html,md}\"",
    "lint:all": "pnpm lint:css && pnpm lint:js && pnpm lint:md && pnpm lint:remark && pnpm lint:format",
    "up": "taze major -I",
    "prepare": "husky install",
    "cm": "cz",
    "docs:build": "vuepress-vite build docs",
    "docs:clean-dev": "vuepress-vite dev docs --clean-cache",
    "docs:dev": "vuepress-vite dev docs",
    "docs:update": "pnpm dlx vp-update",
    "test": "vitest",
    "sizecheck": "cross-env SIZE_CHECK=true pnpm docs:build"
  },
  "dependencies": {
    "@shikijs/vitepress-twoslash": "^1.12.1",
    "cnchar": "^3.2.6",
    "cnchar-draw": "^3.2.6",
    "cnchar-explain": "^3.2.6",
    "cnchar-idiom": "^3.2.6",
    "cnchar-order": "^3.2.6",
    "cnchar-radical": "^3.2.6",
    "cnchar-voice": "^3.2.6",
    "cnchar-words": "^3.2.6",
    "vue": "^3.4.31"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.21.2",
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@commitlint/cz-commitlint": "^19.2.0",
    "@textlint/ast-node-types": "^14.0.4",
    "@textlint/types": "^14.0.4",
    "@types/postcss-import": "^14.0.3",
    "@unocss/eslint-config": "^0.58.9",
    "@vitest/coverage-v8": "^2.0.5",
    "@vue/repl": "^4.3.1",
    "@vue/test-utils": "^2.4.6",
    "@vuepress/bundler-vite": "2.0.0-rc.14",
    "@vuepress/bundler-webpack": "2.0.0-rc.14",
    "@vuepress/plugin-docsearch": "2.0.0-rc.37",
    "@vuepress/plugin-pwa": "2.0.0-rc.37",
    "@vuepress/plugin-seo": "2.0.0-rc.37",
    "@vuepress/plugin-sitemap": "2.0.0-rc.37",
    "@waline/client": "^3.2.6",
    "autoprefixer": "^10.4.19",
    "commitizen": "^4.3.0",
    "critters": "^0.0.20",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "echarts": "^5.5.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-cypress": "^2.15.2",
    "eslint-plugin-format": "^0.1.2",
    "flowchart.ts": "^3.0.0",
    "husky": "^9.0.11",
    "katex": "^0.16.11",
    "lint-staged": "^15.2.7",
    "markdown-it": "^14.1.0",
    "markdown-it-container": "^4.0.0",
    "markdown-it-link-attributes": "^4.0.1",
    "markdown-it-shikiji": "^0.10.2",
    "markdownlint": "^0.33.0",
    "markdownlint-cli": "^0.39.0",
    "mermaid": "^10.9.1",
    "postcss": "^8.4.39",
    "postcss-html": "^1.7.0",
    "postcss-import": "^16.1.0",
    "postcss-scss": "^4.0.9",
    "prettier": "^3.3.2",
    "remark-cli": "^12.0.1",
    "remark-gfm": "^4.0.0",
    "remark-github": "^12.0.0",
    "remark-preset-lint-consistent": "^5.1.2",
    "remark-preset-lint-recommended": "^6.1.3",
    "remark-toc": "^9.0.0",
    "remark-validate-links": "^13.0.1",
    "reveal.js": "^5.1.0",
    "rollup-plugin-visualizer": "^5.12.0",
    "sandpack-vue3": "^3.1.11",
    "sass": "^1.77.6",
    "shikiji": "^0.10.2",
    "stylelint": "^16.6.1",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-hudochenkov": "^10.0.0",
    "stylelint-config-prettier-scss": "^1.0.0",
    "stylelint-config-standard": "^36.0.1",
    "stylelint-order": "^6.0.4",
    "stylelint-scss": "^6.3.2",
    "taze": "^0.13.9",
    "textlint": "^14.0.4",
    "textlint-rule-common-misspellings": "^1.0.1",
    "textlint-rule-no-todo": "^2.0.1",
    "textlint-rule-rousseau": "^1.4.8",
    "textlint-rule-unexpanded-acronym": "^1.2.4",
    "typescript": "^5.5.3",
    "unocss": "^0.58.9",
    "vitest": "^2.0.5",
    "vue-tsc": "^1.8.27",
    "vuepress": "2.0.0-rc.14",
    "vuepress-theme-hope": "2.0.0-rc.50"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@algolia/client-search",
        "react",
        "react-dom",
        "@types/react",
        "webpack"
      ]
    }
  }
}
